<omniFat>  <description>General OmniFAT instance for EJT, with zero tolerance for in-name punctuation for maximum precision on born-digital publications, which do not exhibit the odd punctuation schemes found in legacy documents.</description>  <fuzzyMatch threshold="0"/>  <abbreviationLengthInLetters min="1" max="2" unDotted="false"/>  <newEpithetLabels separator=";">n.;nov.;novus;nova;novum;new</newEpithetLabels>  <intraEpithetPunctuation separator=";"></intraEpithetPunctuation>  <interEpithetPunctuation separator=";"></interEpithetPunctuation>  <dictionary ref="anatomicDircetions.list.txt" type="stopWord" suffixDiffThreshold="0"/>  <dictionary ref="EnglishWord.list.txt" type="negative" suffixDiffThreshold="0"/>  <dictionary ref="ForbiddenWordsLowerCase.list.txt" type="stopWord" suffixDiffThreshold="0"/>  <dictionary ref="ForbiddenWordsUpperCase.list.txt" type="stopWord" suffixDiffThreshold="0"/>  <dictionary ref="GermanWord.list.txt" type="negative" suffixDiffThreshold="0"/>  <dictionary ref="StopWord.list.txt" type="stopWord" suffixDiffThreshold="0"/>  <dictionary ref="stopWordsLatin.list.txt" type="negative" suffixDiffThreshold="0"/>  <pattern string="nov" isRef="false" type="negative"/>  <pattern string=".*ing" isRef="false" type="negative"/>  <pattern string=".*y" isRef="false" type="negative"/>  <pattern string=".*al" isRef="false" type="negative"/>  <pattern string=".*an" isRef="false" type="negative"/>  <pattern string=".*ts" isRef="false" type="negative"/>  <pattern string=".*ds" isRef="false" type="negative"/>  <pattern string=".*nd" isRef="false" type="negative"/>  <pattern string=".*ed" isRef="false" type="negative"/>  <pattern string=".*th" isRef="false" type="negative"/>  <pattern string=".*nt" isRef="false" type="negative"/>  <pattern string=".*ls" isRef="false" type="negative"/>  <pattern string=".*like" isRef="false" type="negative"/>  <pattern string=".*oid" isRef="false" type="negative"/>  <pattern string=".*tic" isRef="false" type="negative"/>  <pattern string=".*uit" isRef="false" type="negative"/>  <pattern string=".*ose" isRef="false" type="negative"/>  <pattern string=".*rs" isRef="false" type="negative"/>  <pattern string=".*tle" isRef="false" type="negative"/>  <pattern string=".*lt" isRef="false" type="negative"/>  <pattern string=".*nce" isRef="false" type="negative"/>  <pattern string=".*ibus" isRef="false" type="negative"/>  <pattern string=".*que" isRef="false" type="negative"/>  <pattern string="[Ff]ig(ure)?s?" isRef="false" type="negative"/>  <pattern string="[Mm]*[Dd]?[Xx]?[Cc]*[Ll]?[Ii]?[Xx]*[Vv]?[Ii]*" isRef="false" type="negative"/>  <pattern string="groupFilterPattern.regEx.txt" isRef="true" type="negative"/>  <pattern string="nymFilterPattern.regEx.txt" isRef="true" type="negative"/>  <pattern string="typeFilterPattern.regEx.txt" isRef="true" type="negative"/>  <stemmingRule separator=";" matchEnding="s"></stemmingRule>  <stemmingRule separator=";" matchEnding="ed">;e</stemmingRule>  <stemmingRule separator=";" matchEnding="ly">;e</stemmingRule>  <stemmingRule separator=";" matchEnding="ies">y</stemmingRule>  <stemmingRule separator=";" matchEnding="ied">y</stemmingRule>  <stemmingRule separator=";" matchEnding="ing">;e</stemmingRule>  <stemmingRule separator=";" matchEnding="ish">;e</stemmingRule>  <stemmingRule separator=";" matchEnding="lly">l;ll</stemmingRule>  <stemmingRule separator=";" matchEnding="er">;e</stemmingRule>  <stemmingRule separator=";" matchEnding="est">;e</stemmingRule>  <rankGroup name="higher" capitalized="always" repeatedEpithets="false" inCombinations="false" doSuffixDiff="false" learningMode="off">    <pattern string="[A-Z][a-z]++" isRef="false" type="recall"/>    <rank name="higher" labeled="never" required="false" epithetDisplayPattern="@epithet" probability="1">      <labels separator=";"></labels>    </rank>    <rank name="phylum" labeled="always" required="false" epithetDisplayPattern="@epithet" probability="1">      <labels separator=";">Phylum;phylum</labels>    </rank>    <rank name="class" labeled="always" required="false" epithetDisplayPattern="@epithet" probability="1">      <labels separator=";">Class;class</labels>    </rank>    <rank name="order" labeled="always" required="false" epithetDisplayPattern="@epithet" probability="1">      <labels separator=";">Order;order</labels>    </rank>  </rankGroup>  <rankGroup name="family" capitalized="always" repeatedEpithets="false" inCombinations="false" doSuffixDiff="false" learningMode="off">    <pattern string=".*an" isRef="false" type="negative"/>    <rank name="family" labeled="sometimes" required="false" epithetDisplayPattern="@epithet" probability="5">      <labels separator=";">Family;family;fam;fam.;familia</labels>      <pattern string="[A-Z][a-z]+idae" isRef="false" type="precision"/>      <pattern string="[A-Z]+IDAE" isRef="false" type="precision"/>    </rank>    <rank name="subFamily" labeled="sometimes" required="false" epithetDisplayPattern="@epithet" probability="5">      <labels separator=";">subfamily;subfam;subfam.;subfamilia;sf.;sfam.</labels>      <pattern string="[A-Z][a-z]+inae" isRef="false" type="precision"/>      <pattern string="[A-Z]+INAE" isRef="false" type="precision"/>    </rank>    <rank name="tribe" labeled="sometimes" required="false" epithetDisplayPattern="@epithet" probability="5">      <labels separator=";">t.;tribe;tribus;tr.</labels>      <pattern string="[A-Z][a-z]+ini" isRef="false" type="precision"/>      <pattern string="[A-Z]+INI" isRef="false" type="precision"/>    </rank>  </rankGroup>  <rankGroup name="genus" capitalized="always" repeatedEpithets="false" inCombinations="true" doSuffixDiff="false" learningMode="teach">    <pattern string="[A-Z][a-z]{3,}" isRef="false" type="recall"/>    <pattern string="[A-Z]{4,}" isRef="false" type="recall"/>    <rank name="genus" labeled="sometimes" required="true" epithetDisplayPattern="@epithet" probability="10">      <labels separator=";">Genus;genus;gen.</labels>    </rank>    <rank name="subGenus" labeled="sometimes" required="false" epithetDisplayPattern="(@epithet)" probability="5">      <labels separator=";">subgenus;subgen.;sgen.</labels>      <pattern string="\([A-Z][a-z]{3,}\)" isRef="false" type="recall"/>    </rank>  </rankGroup>  <rankGroup name="species" capitalized="never" repeatedEpithets="true" inCombinations="true" doSuffixDiff="false" learningMode="teach">    <pattern string="[a-z]{3,}(\-[a-z]{3,})?" isRef="false" type="recall"/>    <pattern string=".*an" isRef="false" type="negative"/>    <rank name="species" labeled="never" required="true" epithetDisplayPattern="@epithet" probability="9">      <labels separator=";">species;sp.</labels>    </rank>    <rank name="subSpecies" labeled="sometimes" required="false" epithetDisplayPattern="subsp. @epithet" probability="6">      <labels separator=";">subspecies;subsp.;ssp.</labels>    </rank>    <rank name="variety" labeled="sometimes" required="false" epithetDisplayPattern="var. @epithet" probability="5">      <labels separator=";">variety;varietas;variatio;var.;v.</labels>    </rank>  </rankGroup>  <authors allowEmbedded="false">    <dictionary ref="ants.authors.static.list.txt"/>    <pattern string="[A-Z]((\.\s)?\-[A-Z])?\." isRef="false" type="part"/>    <pattern string="lastName.regEx.txt" isRef="true" type="positive"/>    <nameStopWords separator="|">of|von|van|von der|van der|von den|van den|dos|del|da|de|do</nameStopWords>    <nameListEndSeparators separator=";">&amp;;and</nameListEndSeparators>    <nameListSeparators separator=";">&amp;</nameListSeparators>  </authors>  <rules cleanupAfter="true">    <ruleSet maxRounds="0">      <rule match="p[plau]+p" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="n+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="un+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="an+" action="remove" removeNested="false" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="p+l" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="p+[plaun]*p+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[bla]p[plau]*p" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[bla]p+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[bp]+u" action="feedback" removeNested="true" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="uu+" action="feedback" removeNested="false" likelyMatch="false"/>      <rule match="[bpl]+[pl][pl]+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[pl]+[plau]*[pl]+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="b+n+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="b+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="u" action="remove" removeNested="false" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="p" action="feedback" removeNested="false" likelyMatch="false"/>      <rule match="[pblua]*n+[blua]*" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="p+l" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="bp+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="p+" action="promote" removeNested="true" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="[pl]{2,}" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[ab]l+" action="feedback" removeNested="true" likelyMatch="true"/>      <rule match="[al]+l" action="feedback" removeNested="true" likelyMatch="false"/>    </ruleSet>  </rules></omniFat>