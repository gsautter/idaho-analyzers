eNested="true" likelyMatch="false"/>      <rule match="n+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="un+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="an+" action="remove" removeNested="false" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="p+l" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="p+u" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="p+[plaun]*p+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[bla]p[plau]*p" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[bla]p+" action="promote" removeNested="true" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="[bpl]+[pl][pl]+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[pl]+[plau]*[pl]+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="b+n+" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="b+" action="remove" removeNested="false" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="[pblua]*n+[blua]*" action="remove" removeNested="false" likelyMatch="false"/>      <rule match="p+l" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="bp+" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="b?up+" action="feedback" removeNested="true" likelyMatch="true"/>      <rule match="p+" action="promote" removeNested="true" likelyMatch="false"/>    </ruleSet>    <ruleSet maxRounds="0">      <rule match="[pl]{2,}" action="promote" removeNested="true" likelyMatch="false"/>      <rule match="[ab]l+" action="feedback" removeNested="true" likelyMatch="true"/>      <rule match="[al]+l" action="feedback" removeNested="true" likelyMatch="false"/>    </ruleSet>  </rules></omniFat>